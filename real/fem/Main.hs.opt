module Main (main) where
import Database
import Vector
import Displacement
import Elemforce
import PrintSource
import Printuvwforce
main = getContents >>= \ s -> process s
 
process :: [Char] -> IO ()
process (!s) = putStr a
  where a = source_data db ++
              uvwresult db uvwres ++ forceresult db frc
        db = (idatabase s, rdatabase s)
        (!uvwres) = uvw db
        frc = forces db uvwres